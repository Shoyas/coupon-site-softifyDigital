    <script>
      var dropdown_down_menu = document.getElementById("dropdown-menu-id-nextPage");

      const shopList = async () => {
        const res = await fetch("http://localhost:8081/allStoresNew");
        const allShop  = await res.json();
        const dropdowns = allShop.slice(0,8)
          .map(
            (shop) =>
              `       
                        <div class="col-md-4 single__shop mx-1 mb-2">
                          <a class="dropdown-item mega-navItem-background" href="/singleShop/${shop._id}">
                            <div
                              class="d-flex justify-content-around align-items-center"
                              style="margin-top: -5px"
                              
                            >
                              <div
                                class="single__shop__logo d-flex mx-2 justify-content-center align-items-center"
                              >
                                <img
                                  class=""
                                  style="width: 80px; height: 50px"
                                  src="${shop.image}"
                                />
                              </div>
                              <div class="single__shop__details p-2">
                                <div> ${shop.name}</div>
                                <div>
                                  <span class="text-success"
                                    ><small
                                      >${shop.coupon[0].couponDetails}</small
                                    ></span
                                  >
                                </div>
                                <div class="text-secondary">
                                  <span>
                                    <small
                                      >${shop.coupon.length} offers</small
                                    ></span
                                  >
                                </div>
                              </div>
                            </div>
                          </a>
                        </div>
                        
              `
              
          )
          
          .join("");
          
        dropdown_down_menu.innerHTML = dropdowns;
        const viewAll = document.createElement("a");
        viewAll.setAttribute("href","/allStores")
        viewAll.innerText("See All")
        dropdown_down_menu.append(viewAll);
      };
      shopList();

    </script>
