      }

      .single__shop {
        border: 1px solid gainsboro;
        height: 100px;
        width: 262px;
        display: inline-block;
      }
      .single__shop:hover {
        box-shadow: -1px -1px 10px 5px rgba(0, 0, 0, 0.2);
      }
      .single__shop a:hover {
        text-decoration: none;
      }

      .single__shop__details {
        border-left: 1px solid gainsboro;
        width: 150px;
        height: 100px;
        color: black;
      }

      .modal1 {
        position: fixed;
        top: 50%;
        left: 50%;
        transform: translate(-50%, -50%) scale(0);
        transition: 200ms ease-in-out;
        border: none;
        z-index: 10;
        background-color: white;
        width: 550px;
        max-width: 80%;
      }
      .modal1.active {
        transform: translate(-50%, -50%) scale(1);
      }
      .modal1-header {
        padding: 10px 15px;
        display: flex;
        flex-direction: row-reverse;
        justify-content: space-between;
        align-items: center;
      }
      .modal1-header .close-button {
        cursor: pointer;
        border: none;
        outline: none;
        background: none;
        font-size: 1.25rem;
        font-weight: bold;
      }
      .modal1-body {
        padding: 10px 15px;
      }
      .modal1-footer{
        background-color: #EFF1F5;
      }
      #overlay {
        position: fixed;
        opacity: 0;
        transition: 200ms ease-in-out;
        top: 0;
        left: 0;
        right: 0;
        bottom: 0;
        background-color: rgba(0, 0, 0, 0.5);
        pointer-events: none;
      }
      #overlay.active {
        opacity: 1;
        pointer-events: all;
      }
      .carousel-item{
        width: 100vw;
        height: 45vh;
        
      }
      .carousel-item img{
        object-fit: scale-down;
        
      }
      .icons{
